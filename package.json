{
  "name": "jsaqa-live-7",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test:api:chrome": "npx cypress run --record --spec \"cypress/integration/api.spec.js\" --group test_api --browser chrome",
    "test:login:chrome": "npx cypress run --record --spec \"cypress/integration/login.spec.js\" --group login --browser chrome",
    "test:movies:chrome": "npx cypress run --record --spec \"cypress/integration/movies.spec.js\" --group movies --browser chrome",
    "test:page:chrome": "npx cypress run --record --spec \"cypress/integration/page.spec.js\" --group page --browser chrome",
    "test:parallel:chrome": "run-p -c test:movies:chrome test:login:chrome test:page:chrome test:api:chrome ",
    "test:api:edge": "npx cypress run --record --spec \"cypress/integration/api.spec.js\" --group test_api --browser edge",
    "test:login:edge": "npx cypress run --record --spec \"cypress/integration/login.spec.js\" --group login --browser edge",
    "test:movies:edge": "npx cypress run --record --spec \"cypress/integration/movies.spec.js\" --group movies --browser edge",
    "test:page:edge": "npx cypress run --record --spec \"cypress/integration/page.spec.js\" --group page --browser edge",
    "test:parallel:edge": "run-p -c test:movies:edge test:login:edge test:page:edge test:api:edge ",
    "test:api:electron": "npx cypress run --record --spec \"cypress/integration/api.spec.js\" --group test_api --browser electron",
    "test:login:electron": "npx cypress run --record --spec \"cypress/integration/login.spec.js\" --group login --browser electron",
    "test:movies:electron": "npx cypress run --record --spec \"cypress/integration/movies.spec.js\" --group movies --browser electron",
    "test:page:electron": "npx cypress run --record --spec \"cypress/integration/page.spec.js\" --group page --browser electron",
    "test:parallel:electron": "run-p -c test:movies:electron test:login:electron test:page:electron test:api:electron",
    "test:parallel:all": "run-p -c test:parallel:edge test:parallel:chrome test:parallel:electron "
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cypress": "^9.7.0"
  },
  "keywords": [],
  "description": "",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
